<jaxb:bindings xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  version="1.0">
  <jaxb:bindings schemaLocation="SRC_CRC_PSM_i2b2_response.xsd">
    <jaxb:bindings node="//xs:complexType[@name='statusType']">
      <jaxb:class name="HiveStatusType"/>
    </jaxb:bindings>
    <jaxb:bindings node="//xs:element[@name='response']">
      <jaxb:class name="HiveResponse"/>
    </jaxb:bindings>
  </jaxb:bindings>
  <jaxb:bindings schemaLocation="SRC_CRC_PSM_i2b2_request.xsd">
    <jaxb:bindings node="//xs:element[@name='request']">
      <jaxb:class name="HiveRequest"/>
    </jaxb:bindings>
  </jaxb:bindings>
</jaxb:bindings>