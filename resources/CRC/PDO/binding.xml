<jaxb:bindings xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  version="1.0">
  <jaxb:bindings schemaLocation="CRC_PDO_QRY_response.xsd">
    <jaxb:bindings node="//xs:complexType[@name='statusType']">
      <jaxb:class name="CRCStatusType"/>
    </jaxb:bindings>
    <jaxb:bindings node="//xs:element[@name='response']">
      <jaxb:class name="CRCResponse"/>
    </jaxb:bindings>
  </jaxb:bindings>
  <jaxb:bindings schemaLocation="CRC_PDO_i2b2_request.xsd">
    <jaxb:bindings node="//xs:element[@name='request']">
      <jaxb:class name="HiveRequest"/>
    </jaxb:bindings>
  </jaxb:bindings>
</jaxb:bindings>